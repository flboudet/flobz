[PackageDefinition]

struct <Rect:
  int x,
  int y,
  int w,
  int h>

struct <GraphicResourceDefinition:
  string key,
  string path,
  Rect   crop>

GraphicResourceDefinition graphic

<initRect: Rect &this, int x, int y, int w, int h>
  &this.x = x
  &this.y = y
  &this.w = w
  &this.h = h

<start_graphic>
  graphic.key = ""
  graphic.path = ""
  initRect::graphic.crop x=0 y=0 w=0 h=0

external <end_graphic>

<PackageDefinition>
  [start_graphic]

